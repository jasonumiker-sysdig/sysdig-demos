global:
  sysdig: 
    accessKey: xxx
    region: au1
    secureAPIToken: xxx
  kspm:
    deploy: true
  clusterConfig:
    name: microk8s-cluster-mac
nodeAnalyzer:
  secure:
    vulnerabilityManagement:
      newEngineOnly: true
  nodeAnalyzer:
    benchmarkRunner:
      deploy: false
    runtimeScanner:
      deploy: false
admissionController:
  enabled: true
  scanner:
    enabled: false
  webhook:
    autoscaling:
      minReplicas: 1
clusterScanner:
  enabled: true
  eveEnabled: true
  scannerMode: "local"
  replicaCount: 1